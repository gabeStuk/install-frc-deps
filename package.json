{
  "name": "install-frc-deps",
  "displayName": "install-frc-deps",
  "description": "Installs FRC deps for 4028",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.1"
  },
  "type": "commonjs",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "install-frc-deps.install-deps",
        "title": "Install Dependencies"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "repository": "https://github.com/gabeStuk/install-frc-deps",
  "devDependencies": {
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "extensionDependencies": [
    "wpilibsuite.vscode-wpilib"
  ],
  "dependencies": {
    "mkdirp": "^3.0.1",
    "node-html-parser": "^6.1.13",
    "vscode-wpilibapi": "^2021.1.1"
  }
}
